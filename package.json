{
  "name": "with-typescript",
  "version": "1.0.0",
  "scripts": {
    "dev": "next -p 3003",
    "build": "next build",
    "start:dev": "next start -p 3003",
    "start": "NODE_ENV=production node server.js",
    "type-check": "tsc",
    "extract": "formatjs extract -- 'components/**/*.tsx' -- 'pages/**/*.tsx' --out-file messages/pl.json",
    "postbuild": "next-sitemap",
    "sitemap": "next-sitemap",
    "prettier": "prettier --config .prettierrc --write './pages/**/*.{ts,tsx,jsx,css,scss,json}' './components/**/*.{ts,tsx,jsx,css,scss,json}' './containers/**/*.{ts,tsx,jsx,css,scss,json}' './context/**/*.{ts,tsx,jsx,css,scss,json}' './scss/**/*.{ts,tsx,jsx,css,scss,json}' './utils/**/*.{ts,tsx,jsx,css,scss,json}'",
    "lint": "eslint . --ext .ts,tsx --ignore-pattern node_modules/ --ignore-pattern .cache --ignore-pattern .next",
    "lint:fix": "eslint . --ext .ts,tsx --ignore-pattern node_modules/ --ignore-pattern .cache --ignore-pattern .next --fix"
  },
  "dependencies": {
    "@fortawesome/fontawesome-pro": "6.0.0-beta1",
    "@fortawesome/fontawesome-svg-core": "^1.2.35",
    "@fortawesome/free-brands-svg-icons": "6.0.0-beta1",
    "@fortawesome/pro-duotone-svg-icons": "6.0.0-beta1",
    "@fortawesome/pro-light-svg-icons": "6.0.0-beta1",
    "@fortawesome/pro-regular-svg-icons": "6.0.0-beta1",
    "@fortawesome/pro-solid-svg-icons": "6.0.0-beta1",
    "@fortawesome/pro-thin-svg-icons": "6.0.0-beta1",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "@react-hook/debounce": "^4.0.0",
    "axios": "^0.21.1",
    "classnames": "^2.3.1",
    "clsx": "^1.1.1",
    "formik": "^2.2.6",
    "jwt-decode": "^3.1.2",
    "next": "^12.0.7",
    "next-seo": "^4.28.1",
    "next-transpile-modules": "^8.0.0",
    "notyf": "^3.9.0",
    "react": "^17.0.2",
    "react-autosuggest": "^10.1.0",
    "react-data-table-component": "^7.4.5",
    "react-dom": "^17.0.2",
    "react-dropzone": "^11.3.2",
    "react-gtm-module": "^2.0.11",
    "react-image-gallery": "^1.1.1",
    "react-quill": "^2.0.0-beta.2",
    "react-upload-gallery": "^0.2.0",
    "styled-components": "^5.3.3",
    "sweetalert2": "^11.2.1",
    "typescript": "^4.3.2"
  },
  "devDependencies": {
    "@formatjs/cli": "^4.2.7",
    "@tailwindcss/jit": "^0.1.18",
    "@types/axios": "^0.14.0",
    "@types/node": "^12.12.21",
    "@types/react": "^17.0.2",
    "@types/react-autosuggest": "^10.1.4",
    "@types/react-dom": "^17.0.1",
    "@types/react-gtm-module": "^2.0.0",
    "@types/react-image-gallery": "^1.0.3",
    "@types/react-popover": "^0.5.3",
    "@types/react-redux": "^7.1.16",
    "@types/redux": "^3.6.0",
    "@types/redux-logger": "^3.0.8",
    "@types/redux-saga": "^0.10.5",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "autoprefixer": "^10.2.5",
    "eslint": "^7.24.0",
    "eslint-config-next": "^11.0.0",
    "eslint-config-prettier": "^8.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "next-sitemap": "^1.6.179",
    "postcss": "^8.2.13",
    "prettier": "^2.2.1",
    "react-intl": "^5.15.8",
    "sass": "^1.32.8",
    "tailwindcss": "^2.1.2"
  },
  "license": "MIT"
}
